<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.hibernate.lexter.Role" table="roletbl">
		<meta attribute="class-description">
			This class contains the roles details.
		</meta>
		<id name="id">
			<generator class="increment">
				<!-- <param name="sequence">roletbl_id_seq</param> -->
			</generator>
		</id>

		<property name="role" column="rolename" type="string"/>
	</class>


</hibernate-mapping>